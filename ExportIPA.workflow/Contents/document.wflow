					<string># Execute

function alert() {
	if [ "$2" == "" ]; then
		osascript -e "display alert \"$1\" as critical" &gt; /dev/null
	else
		osascript -e "display alert \"$1\" message \"$2\" as critical" &gt; /dev/null
	fi
}

target="$1"
name="$2"
if [ "$target" != "" ] &amp;&amp; [ "$name" != "" ]; then
	targetPath=${target%/*}/$name
	result=`xcodebuild -exportArchive -exportFormat ipa -archivePath "$target" -exportWithOriginalSigningIdentity -exportPath "$targetPath.ipa"`
	if [ $? -ne 0 ]; then
		echo "$result" &gt; "$targetPath.txt"
		alert "Export IPA failed. See $name.txt for detail." "$target"
		echo "$targetPath.txt"
	else
		echo "$targetPath.ipa"
	fi
fi
