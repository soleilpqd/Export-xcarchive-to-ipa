					<key>COMMAND_STRING</key>
					<string># Validate inputs

function alert() {
	if [ "$2" == "" ]; then
		osascript -e "display alert \"$1\" as critical" &gt; /dev/null
	else
		osascript -e "display alert \"$1\" message \"$2\" as critical" &gt; /dev/null
	fi
}

target="$1"
if [ "$target" == "" ]; then
	alert "No input!"
elif [ ! -d "$target" ]; then
	alert "Invalid input!" "$target"
else
	echo "$target"
	target=`basename "$target"`
	echo ${target%.*}
fi</string>
